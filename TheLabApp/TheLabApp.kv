            title: "Images"
            ImageExample:
    Screen:
        name: "WidgetsExample"
        BoxLayoutWithActionBar:
            title: "Widgets"
            WidgetsExample:
    Screen:
        name: "LayoutExamplesTabs"
        BoxLayoutWithActionBar:
            title: "Layouts"
            LayoutExamplesTabs:









